{
  "id": null,
  "uid": "workflow-execution",
  "title": "Workflow Execution Observability",
  "description": "Monitor workflow execution performance, traces, and identify bottlenecks. Use this dashboard to see why workflows are failing or slow.",
  "tags": ["workflow", "execution", "traces", "observability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "panels": [
    {
      "id": 1,
      "title": "Workflow Execution Traces",
      "type": "traces",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0},
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "traceql",
          "query": "{service.name=\"rpa-system-backend\" && name=~\"workflow.execute.*\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "spanBarType": "Tag",
        "enableLogsIntegration": true,
        "enableTraceSearch": true
      }
    },
    {
      "id": 2,
      "title": "Execution Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 10},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate(easyflow_span_calls_total{service_name=\"rpa-system-backend\", span_name=~\"workflow.execute.*\"}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "P95 Duration",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 10},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate(easyflow_span_duration_bucket{service_name=\"rpa-system-backend\", span_name=~\"workflow.execute.*\"}[5m])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "P50 Duration",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 10},
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.50, sum(rate(easyflow_span_duration_bucket{service_name=\"rpa-system-backend\", span_name=~\"workflow.execute.*\"}[5m])) by (le))",
