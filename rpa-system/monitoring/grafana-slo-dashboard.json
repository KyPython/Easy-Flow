{
  "dashboard": {
    "id": null,
    "title": "EasyFlow SLO Executive Dashboard",
    "description": "Strategic SLO monitoring for business reliability - tracks process success rate, API latency, and user transaction performance",
    "tags": ["slo", "executive", "business-reliability"],
    "timezone": "UTC",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-28d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "fiscalYearStartMonth": 0,
    "liveNow": true,
    "panels": [
      {
        "id": 1,
        "title": "üéØ SLO Executive Scorecard",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99.9}
              ]
            },
            "custom": {
              "displayMode": "basic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "SLO 1: Process Success Rate"},
              "properties": [
                {"id": "displayName", "value": "üîÑ Process Success Rate (Target: ‚â•99.9%)"},
                {"id": "thresholds", "value": {
                  "mode": "absolute", 
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 99.5},
                    {"color": "green", "value": 99.9}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "SLO 2: API Latency"},
              "properties": [
                {"id": "displayName", "value": "‚ö° API Latency P95 (Target: ‚â•95% <500ms)"},
                {"id": "thresholds", "value": {
                  "mode": "absolute",
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 90},
                    {"color": "green", "value": 95}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "SLO 3: User Transaction"},
              "properties": [
                {"id": "displayName", "value": "üë§ User Transaction P95 (Target: ‚â•95% <3s)"},
                {"id": "thresholds", "value": {
                  "mode": "absolute",
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 90},
                    {"color": "green", "value": 95}
                  ]
                }}
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "easyflow:slo1_success_rate_28d",
            "legendFormat": "SLO 1: Process Success Rate",
            "refId": "A"
          },
          {
            "expr": "easyflow:slo2_latency_success_rate_28d", 
            "legendFormat": "SLO 2: API Latency",
            "refId": "B"
          },
          {
            "expr": "easyflow:slo3_transaction_success_rate_28d",
            "legendFormat": "SLO 3: User Transaction",
            "refId": "C"
          }
        ]
      },
      {
        "id": 2,
        "title": "üö® Error Budget Status",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "vertical",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 25},
                {"color": "green", "value": 50}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "easyflow:slo1_error_budget_remaining",
            "legendFormat": "üîÑ Process Success Budget",
            "refId": "A"
          },
          {
            "expr": "easyflow:slo2_error_budget_remaining",
            "legendFormat": "‚ö° API Latency Budget", 
            "refId": "B"
          },
          {
            "expr": "easyflow:slo3_error_budget_remaining",
            "legendFormat": "üë§ User Transaction Budget",
            "refId": "C"
          }
        ]
      },
      {
        "id": 3,
        "title": "üìä SLO Health Trend (7 Days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "min", "max"]},
          "displayMode": "line"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 95,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "red", "value": 99.9}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "Process.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "API.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "User.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "easyflow:slo1_success_rate_28d",
            "legendFormat": "üîÑ Process Success Rate",
            "refId": "A"
          },
          {
            "expr": "easyflow:slo2_latency_success_rate_28d",
            "legendFormat": "‚ö° API Latency Compliance",
            "refId": "B"
          },
          {
            "expr": "easyflow:slo3_transaction_success_rate_28d", 
            "legendFormat": "üë§ User Transaction Compliance",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "‚ö†Ô∏è Error Budget Burn Rate (Critical Alerting)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "alert": {
          "alertRuleTags": {"team": "sre", "severity": "critical", "slo": "process_execution"},
          "conditions": [
            {
              "evaluator": {"params": [14], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "1m",
          "handler": 1,
          "name": "SLO 1 - Fast Burn Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "table", "placement": "bottom"},
          "displayMode": "line"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 2,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 14}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*fast.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "easyflow:slo1_error_budget_burn_rate_1h * 14.4",
            "legendFormat": "üö® Fast Burn (1h window) - Alert threshold: >14x",
            "refId": "A"
          },
          {
            "expr": "easyflow:slo1_error_budget_burn_rate_6h * 6",
            "legendFormat": "‚ö†Ô∏è Slow Burn (6h window) - Alert threshold: >6x", 
            "refId": "B"
          }
        ]
      },
      {
        "id": 5,
        "title": "üìà Business Context Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 24},
        "collapsed": false
      },
      {
        "id": 6,
        "title": "üë• Process Executions by User Tier",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 25},
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "stacking": {"mode": "normal"}
            }
          }
        },
        "targets": [
          {
            "expr": "sum by (user_tier) (rate(easyflow:rpa_executions_by_tier_5m[5m]))",
            "legendFormat": "{{user_tier}} tier",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "üîó API Latency by Workflow Type",
        "type": "heatmap",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 25},
        "options": {
          "calculate": true,
          "cellGap": 2,
          "cellValues": {"decimals": 0},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 64
          },
          "exemplars": {"color": "rgba(255,0,255,0.7)"},
          "filterValues": {"le": 1e-9},
          "legend": {"show": true},
          "rowsFrame": {"layout": "auto"},
          "tooltip": {"show": true, "yHistogram": false},
          "yAxis": {"decimals": 0, "max": 5000, "min": 0, "unit": "ms"}
        },
        "targets": [
          {
            "expr": "sum by (workflow_id, le) (increase(easyflow:api_latency_by_workflow_type_5m[5m]))",
            "format": "heatmap",
            "legendFormat": "{{workflow_id}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "üéØ User Transactions by Operation",
        "type": "piechart", 
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 25},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "donut",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["percent"]},
          "displayLabels": ["name", "value"]
        },
        "targets": [
          {
            "expr": "sum by (operation) (rate(easyflow:user_transactions_by_operation_5m[5m]))",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "üîç SLO Violation Details",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 31},
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "Timestamp"}]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Current Value"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "decimals", "value": 3},
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "thresholds", "value": {
                  "mode": "absolute",
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 95},
                    {"color": "green", "value": 99.9}
                  ]
                }}
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Value #A": "Process Success Rate",
                "Value #B": "API Latency Compliance", 
                "Value #C": "User Transaction Compliance"
              }
            }
          }
        ],
        "targets": [
          {
            "expr": "easyflow:slo1_success_rate_28d",
            "format": "table",
            "legendFormat": "SLO 1: Process Success",
            "refId": "A"
          },
          {
            "expr": "easyflow:slo2_latency_success_rate_28d",
            "format": "table", 
            "legendFormat": "SLO 2: API Latency",
            "refId": "B"
          },
          {
            "expr": "easyflow:slo3_transaction_success_rate_28d",
            "format": "table",
            "legendFormat": "SLO 3: User Transaction",
            "refId": "C"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\"SLO.*\"}",
          "iconColor": "red",
          "name": "SLO Violations",
          "titleFormat": "üö® {{alertname}}",
          "textFormat": "{{instance}} - {{summary}}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {"selected": false, "text": "All", "value": "$__all"},
          "datasource": "prometheus",
          "definition": "label_values(easyflow:rpa_executions_by_tier_5m, user_tier)",
          "hide": 0,
          "includeAll": true,
          "name": "user_tier",
          "options": [],
          "query": {
            "query": "label_values(easyflow:rpa_executions_by_tier_5m, user_tier)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        },
        {
          "current": {"selected": false, "text": "28d", "value": "28d"},
          "hide": 0,
          "includeAll": false,
          "name": "slo_window",
          "options": [
            {"selected": false, "text": "7d", "value": "7d"},
            {"selected": true, "text": "28d", "value": "28d"},
            {"selected": false, "text": "90d", "value": "90d"}
          ],
          "query": "7d,28d,90d",
          "queryValue": "",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    }
  }
}