{
  "version": 2,
  "name": "easyflow-dashboard",
  "framework": null,
  "installCommand": "npm ci",
  "buildCommand": "node node_modules/react-app-rewired/scripts/build.js",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}