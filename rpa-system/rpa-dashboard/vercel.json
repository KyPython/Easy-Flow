{
  "version": 2,
  "name": "easyflow-dashboard",
  "builds": [
    {
      "src": "rpa-system/rpa-dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd rpa-system/rpa-dashboard && npm ci && npm run build",
  "outputDirectory": "rpa-system/rpa-dashboard/build",
  "installCommand": "cd rpa-system && npm ci"
}