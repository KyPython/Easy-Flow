{
  "name": "rpa-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.tryeasyflow.com",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-web": "^0.35.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/instrumentation-document-load": "^0.35.0",
    "@opentelemetry/instrumentation-fetch": "^0.46.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.35.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.46.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-trace-web": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "@supabase/supabase-js": "^2.45.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@vercel/analytics": "^1.0.0",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.4",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "prebuild": "node vercel-scripts/generate-firebase-config.js",
    "prestart": "node vercel-scripts/generate-firebase-config.js",
    "start": "react-app-rewired start",
    "dev": "react-app-rewired start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "postinstall": "node -e \"if (process.env.CI || process.env.VERCEL) { process.exit(0); } else { try { require('child_process').execSync('npx patch-package', { stdio: 'inherit' }); } catch(e) { process.exit(0); } }\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "accessibility:test": "npm run build && npx serve -s build -l 3000 & sleep 5 && npx pa11y-ci --config .pa11yci.json",
    "accessibility:axe": "npm run build && npx @axe-core/cli http://localhost:3000 --tags wcag2a,wcag2aa,wcag21aa --save results/axe-report.json",
    "accessibility:all": "npm run accessibility:test && npm run accessibility:axe",
    "e2e": "cypress run"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/cli": "^4.9.0",
    "@eslint/js": "^9.34.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "baseline-browser-mapping": "^2.9.19",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "customize-cra": "^1.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "http-proxy-middleware": "^3.0.5",
    "os-browserify": "^0.3.0",
    "pa11y": "^7.0.0",
    "pa11y-ci": "^3.0.1",
    "patch-package": "^8.0.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "querystring-es3": "^0.2.1",
    "react-app-rewired": "^2.2.1",
    "serve": "^14.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.9.5",
    "url": "^0.11.4",
    "util": "^0.12.5"
  }
}
