{
  "version": 2,
  "name": "easyflow-dashboard",
  "framework": null,
  "buildCommand": "npm ci --prefix rpa-system/rpa-dashboard && npm run build --prefix rpa-system/rpa-dashboard",
  "outputDirectory": "rpa-system/rpa-dashboard/build",
  "installCommand": null,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}